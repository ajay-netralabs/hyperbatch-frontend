    "@uiw/react-textarea-code-editor": "^3.0.2",
    "download.js": "^1.0.0",
    "draft-js": "^0.11.7",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "universal-cookie": "^7.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
