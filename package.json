{
  "name": "hyperbatch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/App.css -o ./src/index.css && vite",
    "build": "tsc && npx tailwindcss -i ./src/App.css -o ./src/index.css && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tailwind:build": "npx tailwindcss -i ./src/App.css -o ./src/index.css",
    "tailwind": "npx tailwindcss -i ./src/App.css -o ./src/index.css --watch"
  },
